							tr
								th width="10%": input type="checkbox"
								th width="10%" 題型
								th 題目
								th width="15%" 建立日期
							// ture or else
							== render "partials/quiz_list_pop_asset" , :locals => {:f => "ture"}
			._afb_footbar_out
				._afb_footbar
					.center-block
						.fr
							input.btn.btn-green<> type="button" value="儲存" onclick="javascript:parent.jQuery.fancybox.close();"
							input.btn.btn-red<> type="button" value="取消" onclick="javascript:parent.jQuery.fancybox.close();"